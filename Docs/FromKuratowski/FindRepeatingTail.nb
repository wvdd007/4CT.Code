Cell[BoxData[
 RowBox[{"Module", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"MAX", "=", "1500000"}], ",", "val", ",", "pow", ",", 
     RowBox[{"three", "=", "3"}], ",", "current", ",", "last", ",", 
     "newPow"}], "}"}], ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"last", " ", "=", " ", "9"}], ";", "\[IndentingNewLine]", 
    RowBox[{"pow", "=", "0"}], ";", "\[IndentingNewLine]", 
    RowBox[{"Monitor", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"pow", "\[LessEqual]", "MAX"}], " ", "&&", " ", 
         RowBox[{"last", " ", "\[LessEqual]", " ", "MAX"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"three", "=", 
          RowBox[{"3", "^", "pow"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"For", "[", 
          RowBox[{
           RowBox[{"current", "=", "last"}], ",", " ", 
           RowBox[{"current", " ", "\[LessEqual]", " ", "MAX"}], ",", " ", 
           RowBox[{"current", " ", "++"}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"val", " ", "=", 
             RowBox[{"2", "^", "current"}]}], ";", "\[IndentingNewLine]", 
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"DigitCount", "[", 
                RowBox[{
                 RowBox[{"Mod", "[", 
                  RowBox[{"val", ",", "three"}], "]"}], ",", "3", ",", "2"}], 
                "]"}], " ", "\[Equal]", "0"}], ",", " ", 
              "\[IndentingNewLine]", 
              RowBox[{
               RowBox[{"Print", "[", 
                RowBox[{"\"\<Found : \>\"", " ", "<>", " ", 
                 RowBox[{"ToString", "[", "current", "]"}]}], "]"}], ";", 
               "\[IndentingNewLine]", 
               RowBox[{"last", "=", "current"}], ";", "\[IndentingNewLine]", 
               RowBox[{"newPow", " ", "=", 
                RowBox[{"IntegerLength", "[", 
                 RowBox[{"val", ",", "3"}], "]"}]}], ";", " ", 
               "\[IndentingNewLine]", 
               RowBox[{"If", "[", 
                RowBox[{
                 RowBox[{"newPow", "\[Equal]", "pow"}], ",", 
                 "\[IndentingNewLine]", 
                 RowBox[{
                  RowBox[{"Print", "[", "\"\<Same power found\>\"", "]"}], 
                  ";", "\[IndentingNewLine]", 
                  RowBox[{"pow", "=", " ", 
                   RowBox[{"pow", " ", "+", "1"}]}]}], "\[IndentingNewLine]", 
                 ",", "\[IndentingNewLine]", 
                 RowBox[{
                  RowBox[{"pow", "=", 
                   RowBox[{"IntegerLength", "[", 
                    RowBox[{"val", ",", "3"}], "]"}]}], ";"}]}], 
                "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
               RowBox[{"Print", "[", 
                RowBox[{"\"\<Going to power : \>\"", " ", "<>", " ", 
                 RowBox[{"ToString", "[", "pow", "]"}]}], "]"}], ";", 
               "\[IndentingNewLine]", 
               RowBox[{"Break", "[", "]"}], ";"}]}], "\[IndentingNewLine]", 
             "]"}], ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"current", "\[GreaterEqual]", " ", "MAX"}], ",", " ", 
           RowBox[{"Break", "[", "]"}]}], "]"}], ";"}]}], 
       "\[IndentingNewLine]", "]"}], ",", "\[IndentingNewLine]", 
      RowBox[{"{", 
