Cell[TextData[StyleBox["The cyclemat computes a matrix.  Each row corresponds \
to a vertex.  Each column k corresponds to the number of cycles of length k \
in which that vertex participates.  For the moment it turned out to be of \
limited usage.", "ItemParagraph"]], "Subsection",
 CellChangeTimes->{{3.6562430490630937`*^9, 3.656243156850236*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"CycleMat", "[", "g_", "]"}], ":=", 
  RowBox[{"Block", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"cycles", ",", 
      RowBox[{"vertexCount", "=", 
       RowBox[{"VertexCount", "[", "g", "]"}]}], ",", "result", ",", " ", "k",
       ",", "l", ",", "m", ",", " ", "current", ",", "currentLength", ",", 
      " ", "first", ",", " ", "second", ",", " ", "pair"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"cycles", "=", 
      RowBox[{"FindCycle", "[", 
       RowBox[{"g", ",", 
        RowBox[{"{", 
         RowBox[{"2", ",", "vertexCount"}], "}"}], ",", "All"}], "]"}]}], ";",
      "\[IndentingNewLine]", 
     RowBox[{"result", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"Table", "[", 
         RowBox[{"0", ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "1", ",", "vertexCount"}], "}"}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", "1", ",", "vertexCount"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"l", "=", "1"}], ",", 
       RowBox[{"l", "\[LessEqual]", 
        RowBox[{"Length", "[", "cycles", "]"}]}], ",", 
       RowBox[{"l", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"current", " ", "=", " ", 
         RowBox[{"cycles", "[", 
          RowBox[{"[", "l", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"currentLength", "=", 
         RowBox[{"Length", "[", "current", "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"For", "[", 
         RowBox[{
          RowBox[{"m", "=", "1"}], ",", 
          RowBox[{"m", "\[LessEqual]", "currentLength"}], ",", 
          RowBox[{"m", "++"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"pair", "=", 
            RowBox[{"current", "[", 
             RowBox[{"[", "m", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"first", "=", 
            RowBox[{"pair", "[", 
             RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"second", "=", 
            RowBox[{"pair", "[", 
             RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"result", "[", 
             RowBox[{"[", 
              RowBox[{"first", ",", "currentLength"}], "]"}], "]"}], "++"}], 
           ";", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"result", "[", 
             RowBox[{"[", 
              RowBox[{"second", ",", "currentLength"}], "]"}], "]"}], "++"}], 
           ";"}]}], "\[IndentingNewLine]", "]"}]}]}], "\[IndentingNewLine]", 
      "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"result", "/", "2"}]}]}], " ", 
   RowBox[{"(*", " ", 
    RowBox[{"each", " ", "vertex", " ", "counted", " ", "twice"}], " ", 
    "*)"}], "\[IndentingNewLine]", "]"}]}]], "Input"]
}, Open  ]],

