Cell[BoxData[
 RowBox[{
  RowBox[{"VertexCycleVect", "[", 
   RowBox[{"g_", ",", " ", "vertex1_", ",", " ", "vertex2_"}], "]"}], ":=", 
  RowBox[{"Block", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"cycles", ",", 
      RowBox[{"vertexCount", "=", 
       RowBox[{"VertexCount", "[", "g", "]"}]}], ",", "result", ",", 
