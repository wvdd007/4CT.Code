  RowBox[{"RepeatDigitsHead", "[", "n_", "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"startPattern", ",", "  ", 
      RowBox[{"start", " ", "=", " ", "0"}], ",", " ", "current", ",", " ", 
