Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"shareQ", "[", 
     RowBox[{"set1_", ",", "set2_"}], "]"}], ":=", 
    RowBox[{
     RowBox[{"Length", "@", 
      RowBox[{"Intersection", "[", 
       RowBox[{"set1", ",", "set2"}], "]"}]}], ">", "0"}]}], ";"}], 
  "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"connecting", " ", "all", " ", "faces"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"innerDualEdges", "[", "faces_", "]"}], ":=", 
   RowBox[{"Join", "@@", 
    RowBox[{"Table", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"shareQ", "@@", 
           RowBox[{"faces", "\[LeftDoubleBracket]", 
            RowBox[{"{", 
             RowBox[{"i", ",", "j"}], "}"}], "\[RightDoubleBracket]"}]}], ",", 
          RowBox[{"UndirectedEdge", "[", 
           RowBox[{"i", ",", "j"}], "]"}], ",", 
          RowBox[{"Unevaluated", "@", 
           RowBox[{"Sequence", "[", "]"}]}]}], "]"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"{", 
         RowBox[{"j", ",", 
          RowBox[{"i", "+", "1"}], ",", 
          RowBox[{"Length", "[", "faces", "]"}]}], "}"}]}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", "1", ",", 
        RowBox[{
         RowBox[{"Length", "[", "faces", "]"}], "-", "1"}]}], "}"}]}], 
     "]"}]}]}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{
   "next", " ", "two", " ", "functions", " ", "find", " ", "the", " ", 
    "faces", " ", "on", " ", "the", " ", "boundary"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"boundaryEdges", "[", "faces_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"edges", "=", 
      RowBox[{"Join", "@@", "faces"}]}], "}"}], ",", 
    RowBox[{"Select", "[", 
     RowBox[{
      RowBox[{"Union", "[", "edges", "]"}], ",", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"Count", "[", 
          RowBox[{"edges", ",", "#"}], "]"}], "\[Equal]", "1"}], ")"}], 
       "&"}]}], "]"}]}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"boundaryFaces", "[", 
     RowBox[{"faces_", ",", "outerEdges_"}], "]"}], ":=", 
    RowBox[{"Select", "[", 
     RowBox[{
      RowBox[{"Range", "@", 
       RowBox[{"Length", "@", "faces"}]}], ",", 
      RowBox[{
       RowBox[{"shareQ", "[", 
        RowBox[{
         RowBox[{
         "faces", "\[LeftDoubleBracket]", "#", "\[RightDoubleBracket]"}], ",",
          "outerEdges"}], "]"}], "&"}]}], "]"}]}], ";"}], 
  "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{"*", " ", "alltogather"}], " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"DualGraph", "[", "faces_", "]"}], ":=", 
  RowBox[{"Graph", "[", 
   RowBox[{"DeleteDuplicates", "@", 
    RowBox[{"Join", "[", 
     RowBox[{
      RowBox[{"Thread", "@", 
       RowBox[{"UndirectedEdge", "[", 
        RowBox[{"0", ",", 
         RowBox[{"boundaryFaces", "[", 
          RowBox[{"faces", ",", 
           RowBox[{"boundaryEdges", "[", "faces", "]"}]}], "]"}]}], "]"}]}], 
      ",", 
      RowBox[{"innerDualEdges", "[", "faces", "]"}]}], "]"}]}], 
   "]"}]}]}], "Input",
