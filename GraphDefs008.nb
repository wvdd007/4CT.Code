        RowBox[{"{", 
         RowBox[{"i", ",", "len"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"current", "=", 
      RowBox[{"len", "+", "1"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"prevInner", "=", "len"}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"inner", "=", "1"}], ",", 
       RowBox[{"inner", "\[LessEqual]", "len"}], ",", 
       RowBox[{"inner", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"For", "[", 
         RowBox[{
          RowBox[{"zone", "=", "0"}], ",", 
          RowBox[{"zone", "\[LessEqual]", 
           RowBox[{"counts", "[", 
            RowBox[{"[", "inner", "]"}], "]"}]}], ",", 
          RowBox[{"zone", "++"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"AppendTo", "[", 
            RowBox[{"result", ",", 
             RowBox[{"current", "\[UndirectedEdge]", "inner"}]}], "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"previous", " ", "\[NotEqual]", 
