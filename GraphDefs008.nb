             RowBox[{
              RowBox[{"AppendTo", "[", 
               RowBox[{"result", ",", 
                RowBox[{"current", "\[UndirectedEdge]", "previous"}]}], "]"}],
               ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"prevInner", " ", "\[NotEqual]", 
              RowBox[{"-", "1"}]}], ",", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"AppendTo", "[", 
               RowBox[{"result", ",", 
