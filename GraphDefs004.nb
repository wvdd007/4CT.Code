Cell[CellGroupData[{

Cell[TextData[StyleBox["The cyclemat computes a matrix.  Each row corresponds \
to a vertex.  Each column k corresponds to the number of cycles of length k \
in which that vertex participates.  For the moment it turned out to be of \
limited usage.", "ItemParagraph"]], "Subsection",
 CellChangeTimes->{{3.6562430490630937`*^9, 3.656243156850236*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"CycleMat", "[", "g_", "]"}], ":=", 
  RowBox[{"Block", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"cycles", ",", 
      RowBox[{"vertexCount", "=", 
       RowBox[{"VertexCount", "[", "g", "]"}]}], ",", "result", ",", " ", "k",
       ",", "l", ",", "m", ",", " ", "current", ",", "currentLength", ",", 
      " ", "first", ",", " ", "second", ",", " ", "pair"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"cycles", "=", 
      RowBox[{"FindCycle", "[", 
       RowBox[{"g", ",", 
        RowBox[{"{", 
         RowBox[{"2", ",", "vertexCount"}], "}"}], ",", "All"}], "]"}]}], ";",
      "\[IndentingNewLine]", 
     RowBox[{"result", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"Table", "[", 
         RowBox[{"0", ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "1", ",", "vertexCount"}], "}"}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", "1", ",", "vertexCount"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"l", "=", "1"}], ",", 
       RowBox[{"l", "\[LessEqual]", 
        RowBox[{"Length", "[", "cycles", "]"}]}], ",", 
       RowBox[{"l", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"current", " ", "=", " ", 
         RowBox[{"cycles", "[", 
          RowBox[{"[", "l", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"currentLength", "=", 
         RowBox[{"Length", "[", "current", "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"For", "[", 
         RowBox[{
          RowBox[{"m", "=", "1"}], ",", 
          RowBox[{"m", "\[LessEqual]", "currentLength"}], ",", 
          RowBox[{"m", "++"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"pair", "=", 
            RowBox[{"current", "[", 
             RowBox[{"[", "m", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"first", "=", 
            RowBox[{"pair", "[", 
             RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"second", "=", 
            RowBox[{"pair", "[", 
             RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"result", "[", 
             RowBox[{"[", 
              RowBox[{"first", ",", "currentLength"}], "]"}], "]"}], "++"}], 
           ";", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"result", "[", 
             RowBox[{"[", 
              RowBox[{"second", ",", "currentLength"}], "]"}], "]"}], "++"}], 
           ";"}]}], "\[IndentingNewLine]", "]"}]}]}], "\[IndentingNewLine]", 
      "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"result", "/", "2"}]}]}], " ", 
   RowBox[{"(*", " ", 
    RowBox[{"each", " ", "vertex", " ", "counted", " ", "twice"}], " ", 
    "*)"}], "\[IndentingNewLine]", "]"}]}]], "Input"]
}, Open  ]],

Cell[CellGroupData[{

Cell[TextData[StyleBox["The edgecyclemat computes an association.  The key of \
the association are the edges of the input graph.  For each of them we \
compute a vector.  The kth value of the vector is the number of k-length \
cycles in which that edge participates.", "ItemParagraph"]], "Subsection",
 CellChangeTimes->{{3.6562430490630937`*^9, 3.656243156850236*^9}, {
  3.656243199362012*^9, 3.656243281344554*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"EdgeCycleMat", "[", "g_", "]"}], ":=", 
  RowBox[{"Block", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"cycles", ",", 
      RowBox[{"vertexCount", "=", 
       RowBox[{"VertexCount", "[", "g", "]"}]}], ",", "result", ",", " ", "k",
       ",", "l", ",", "m", ",", " ", "current", ",", "currentLength", ",", 
      " ", "line", ",", " ", "pair"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"cycles", "=", 
      RowBox[{"FindCycle", "[", 
       RowBox[{"g", ",", 
        RowBox[{"{", 
         RowBox[{"2", ",", "vertexCount"}], "}"}], ",", "All"}], "]"}]}], ";",
      "\[IndentingNewLine]", 
     RowBox[{"result", "=", 
      RowBox[{"Association", "[", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Sort", "[", "e", "]"}], "->", 
          RowBox[{"Table", "[", 
           RowBox[{"0", ",", 
            RowBox[{"{", 
             RowBox[{"i", ",", "1", ",", "vertexCount"}], "}"}]}], "]"}]}], 
         ",", 
         RowBox[{"{", 
          RowBox[{"e", ",", 
           RowBox[{"EdgeList", "[", "g", "]"}]}], "}"}]}], "]"}], "]"}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"l", "=", "1"}], ",", 
       RowBox[{"l", "\[LessEqual]", 
        RowBox[{"Length", "[", "cycles", "]"}]}], ",", 
       RowBox[{"l", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"current", " ", "=", " ", 
         RowBox[{"cycles", "[", 
          RowBox[{"[", "l", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"currentLength", "=", 
         RowBox[{"Length", "[", "current", "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"For", "[", 
         RowBox[{
          RowBox[{"m", "=", "1"}], ",", 
          RowBox[{"m", "\[LessEqual]", "currentLength"}], ",", 
          RowBox[{"m", "++"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"pair", "=", 
            RowBox[{"Sort", "[", 
             RowBox[{"current", "[", 
              RowBox[{"[", "m", "]"}], "]"}], "]"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"line", "=", 
            RowBox[{"result", "[", "pair", "]"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"line", "[", 
             RowBox[{"[", "currentLength", "]"}], "]"}], "++"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"result", "[", "pair", "]"}], "=", "line"}]}]}], 
         "\[IndentingNewLine]", "]"}]}]}], "\[IndentingNewLine]", "]"}], ";", 
     "\[IndentingNewLine]", "result"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{3.656243197355158*^9}]
}, Open  ]],

Cell[CellGroupData[{

Cell[TextData[StyleBox["The vertexcyclevect computes a vector for the given \
vertices.  The kth value of the vector is the number of k-length cycles in \
which both vertices participate.", "ItemParagraph"]], "Subsection",
 CellChangeTimes->{{3.6562430490630937`*^9, 3.656243156850236*^9}, {
  3.656243199362012*^9, 3.656243281344554*^9}, {3.656243317192574*^9, 
  3.6562433582754145`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"VertexCycleVect", "[", 
   RowBox[{"g_", ",", " ", "vertex1_", ",", " ", "vertex2_"}], "]"}], ":=", 
  RowBox[{"Block", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"cycles", ",", 
      RowBox[{"vertexCount", "=", 
       RowBox[{"VertexCount", "[", "g", "]"}]}], ",", "result", ",", 
      "result2", ",", " ", "k", ",", "l", ",", "m", ",", " ", "current", ",", 
      "currentLength"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"cycles", "=", 
      RowBox[{"FindCycle", "[", 
       RowBox[{"g", ",", 
        RowBox[{"{", 
         RowBox[{"2", ",", "vertexCount"}], "}"}], ",", "All"}], "]"}]}], ";",
      "\[IndentingNewLine]", 
     RowBox[{"result", "=", 
      RowBox[{"Table", "[", 
       RowBox[{"0", ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "vertexCount"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"result2", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"{", "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "vertexCount"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"l", "=", "1"}], ",", 
       RowBox[{"l", "\[LessEqual]", 
        RowBox[{"Length", "[", "cycles", "]"}]}], ",", 
       RowBox[{"l", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"current", " ", "=", " ", 
         RowBox[{"cycles", "[", 
          RowBox[{"[", "l", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"MemberQ", "[", 
             RowBox[{"current", ",", 
              RowBox[{"vertex1", "\[UndirectedEdge]", "_"}]}], "]"}], ")"}], "&&", 
           RowBox[{"(", 
            RowBox[{"MemberQ", "[", 
             RowBox[{"current", ",", 
              RowBox[{"vertex2", "\[UndirectedEdge]", "_"}]}], "]"}], ")"}]}],
           ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"currentLength", "=", 
            RowBox[{"Length", "[", "current", "]"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"result", "[", 
             RowBox[{"[", "currentLength", "]"}], "]"}], "++"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"AppendTo", "[", 
            RowBox[{
             RowBox[{"result2", "[", 
              RowBox[{"[", "currentLength", "]"}], "]"}], ",", "current"}], 
            "]"}], ";"}]}], "\[IndentingNewLine]", "]"}]}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", "result"}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{3.656243197355158*^9, 3.6562433129875345`*^9}]
}, Open  ]],

Cell[CellGroupData[{

Cell[TextData[StyleBox["The following takes as input a list as generated by \
rule2.  Elements look like {7,21,\[CloseCurlyDoubleQuote]{2,2<->6,3<->5}\
\[CloseCurlyDoubleQuote]}.  This means that there is a rule 2 transition from \
graph 7 to graph 21.  The existing edge on graph 7 on which we apply the rule \
is 2<->6.  3<->5 are the other vertices (not connected) in the rule 2 \
transition.  For each of them we computed the vertexcyclevect.", \
"ItemParagraph"]], "Subsection",
 CellChangeTimes->{{3.6562430490630937`*^9, 3.656243156850236*^9}, {
  3.656243199362012*^9, 3.656243281344554*^9}, {3.656243317192574*^9, 
