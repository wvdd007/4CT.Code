           "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"line", "[", 
             RowBox[{"[", "currentLength", "]"}], "]"}], "++"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"result", "[", "pair", "]"}], "=", "line"}]}]}], 
         "\[IndentingNewLine]", "]"}]}]}], "\[IndentingNewLine]", "]"}], ";", 
     "\[IndentingNewLine]", "result"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
