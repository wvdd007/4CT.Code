Cell[BoxData[
 RowBox[{
  RowBox[{"RepeatDigitsCarry", "[", 
   RowBox[{"n_", ",", "p_", ",", "q_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"startPattern", ",", "  ", 
      RowBox[{"start", " ", "=", " ", "0"}], ",", " ", "current", ",", " ", 
      "digits", ",", " ", "bad", ",", " ", "tail"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"digits", " ", "=", " ", 
      RowBox[{"CarriesVector2", "[", 
       RowBox[{
        RowBox[{"p", "^", "0"}], ",", "p", ",", "q"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"startPattern", " ", "=", " ", 
      RowBox[{"Take", "[", 
       RowBox[{
        RowBox[{"PadLeft", "[", 
         RowBox[{"digits", ",", "n"}], "]"}], ",", 
        RowBox[{"-", "n"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<Start pattern \>\"", " ", "<>", " ", 
       RowBox[{"ToString", "[", "startPattern", "]"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"current", " ", "=", " ", 
      RowBox[{"start", " ", "+", "1"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"digits", " ", "=", " ", 
      RowBox[{"CarriesVector2", "[", 
       RowBox[{
        RowBox[{"p", "^", "current"}], ",", "p", ",", "q"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"tail", " ", "=", " ", 
      RowBox[{"Take", "[", 
       RowBox[{
        RowBox[{"PadLeft", "[", 
         RowBox[{"digits", ",", "n"}], "]"}], ",", 
        RowBox[{"-", "n"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"bad", "=", "0"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<digit - \>\"", " ", "<>", " ", 
       RowBox[{"ToString", "[", "current", "]"}], "<>", " ", "\"\< --> \>\"", 
       " ", "<>", " ", 
       RowBox[{"ToString", "[", "tail", "]"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"tail", " ", "\[NotEqual]", " ", "startPattern"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"current", " ", "+=", "1"}], ";", "\[IndentingNewLine]", 
        RowBox[{"digits", " ", "=", " ", 
         RowBox[{"CarriesVector2", "[", 
          RowBox[{
           RowBox[{"p", "^", "current"}], ",", "p", ",", "q"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"tail", " ", "=", " ", 
         RowBox[{"Take", "[", 
          RowBox[{
           RowBox[{"PadLeft", "[", 
            RowBox[{"digits", ",", "n"}], "]"}], ",", 
           RowBox[{"-", "n"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"Print", "[", 
         RowBox[{"\"\<digit - \>\"", " ", "<>", " ", 
          RowBox[{"ToString", "[", "current", "]"}], "<>", " ", 
          "\"\< --> \>\"", " ", "<>", " ", 
          RowBox[{"ToString", "[", "tail", "]"}]}], "]"}], ";"}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<Found repeating pattern after \>\"", " ", "<>", " ", 
       RowBox[{"ToString", "[", 
        RowBox[{"current", "-", " ", "start", "+", "1"}], "]"}]}], "]"}], ";",
      "\[IndentingNewLine]", 
     RowBox[{"Return", "[", 
      RowBox[{"current", "-", "start"}], "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
