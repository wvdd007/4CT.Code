            RowBox[{"[", "2", "]"}], "]"}], "]"}]}], "]"}]}], ";"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"PrintTemporary", "[", "\"\<Solving g..\>\"", "]"}], ";", 
     "\[IndentingNewLine]", 
