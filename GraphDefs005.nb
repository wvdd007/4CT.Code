Cell[BoxData[
 RowBox[{
  RowBox[{"MakeInnerOuterGraph", "[", "counts_", "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"len", "=", 
       RowBox[{"Length", "[", "counts", "]"}]}], ",", "result", ",", " ", 
      "current", ",", 
      RowBox[{"previous", "=", 
       RowBox[{"-", "1"}]}], ",", " ", 
      RowBox[{"inner", "=", "1"}], ",", "zone", ",", "prevInner"}], "}"}], 
    ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"result", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"i", "<->", 
         RowBox[{"Succ", "[", 
          RowBox[{"i", ",", "len"}], "]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "len"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"current", "=", 
      RowBox[{"len", "+", "1"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"prevInner", "=", "len"}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"inner", "=", "1"}], ",", 
       RowBox[{"inner", "\[LessEqual]", "len"}], ",", 
       RowBox[{"inner", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"For", "[", 
         RowBox[{
          RowBox[{"zone", "=", "0"}], ",", 
          RowBox[{"zone", "\[LessEqual]", 
           RowBox[{"counts", "[", 
            RowBox[{"[", "inner", "]"}], "]"}]}], ",", 
          RowBox[{"zone", "++"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"AppendTo", "[", 
            RowBox[{"result", ",", 
             RowBox[{"current", "\[UndirectedEdge]", "inner"}]}], "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"previous", " ", "\[NotEqual]", 
              RowBox[{"-", "1"}]}], ",", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"AppendTo", "[", 
               RowBox[{"result", ",", 
                RowBox[{"current", "\[UndirectedEdge]", "previous"}]}], "]"}],
               ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"prevInner", " ", "\[NotEqual]", 
              RowBox[{"-", "1"}]}], ",", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"AppendTo", "[", 
               RowBox[{"result", ",", 
                RowBox[{"current", "\[UndirectedEdge]", "prevInner"}]}], 
               "]"}], ";", "\[IndentingNewLine]", 
              RowBox[{"prevInner", "=", 
               RowBox[{"-", "1"}]}], ";"}]}], "\[IndentingNewLine]", "]"}], 
           ";", "\[IndentingNewLine]", 
           RowBox[{"previous", "=", "current"}], ";", "\[IndentingNewLine]", 
           RowBox[{"current", "++"}], ";"}]}], "\[IndentingNewLine]", "]"}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{"prevInner", "=", "inner"}], ";"}]}], "\[IndentingNewLine]", 
      "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"AppendTo", "[", 
      RowBox[{"result", ",", 
       RowBox[{
        RowBox[{"current", "-", "1"}], "\[UndirectedEdge]", 
        RowBox[{"len", "+", "1"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"AppendTo", "[", 
        RowBox[{"result", ",", 
         RowBox[{
          RowBox[{"current", "-", "1"}], "\[UndirectedEdge]", "1"}]}], "]"}], 
       ";"}], "*)"}], "\[IndentingNewLine]", "result"}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.6618672819733458`*^9, 3.6618672843332753`*^9}, {
