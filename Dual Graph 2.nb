    RowBox[{"UndirectedEdge", "@@@", 
     RowBox[{"Partition", "[", 
      RowBox[{"list", ",", "2", ",", "1"}], "]"}]}]}]}], 
  "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"FindFace", " ", ":", " ", 
