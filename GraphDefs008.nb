  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"len", "=", 
       RowBox[{"Length", "[", "counts", "]"}]}], ",", "result", ",", " ", 
      "current", ",", 
      RowBox[{"previous", "=", 
       RowBox[{"-", "1"}]}], ",", " ", 
      RowBox[{"inner", "=", "1"}], ",", "zone", ",", "prevInner"}], "}"}], 
    ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"result", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"i", "<->", 
